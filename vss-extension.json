{
  "manifestVersion": 1,
  "id": "build-tasks",
  "name": "Tothegist Pipeline Tasks",
  "version": "1.0.0",
  "publisher": "tothegist",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "An Azure Pipeline Task to automate releases with Changesets",
  "categories": [
    "Azure Pipelines"
  ],
  "files": [
    {
      "path": "packages/changesets"
    }
  ],
  "icons": {
    "default": "icon.png"
  },
  "contributions": [
    {
      "id": "changesets-release-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "packages/changesets"
      }
    }
  ]
}
