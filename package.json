    "@changesets/cli": "^2.24.4",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "esbuild": "^0.15.10",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "latest",
    "eslint-plugin-prettier": "latest",
    "prettier": "latest",
    "tfx-cli": "^0.12.0",
    "turbo": "latest",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=10.0.0 <11.0.0"
  },
  "packageManager": "pnpm@7.9.5"
