{
  "name": "root",
  "version": "0.0.0",
  "author": "leumasic <leumasic@outlook.com>",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "eslint .",
